    <LinearLayout
        android:layout_width="410dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/mycomplaints"
            android:layout_width="190dp"
            android:layout_height="90dp"
            android:background="@drawable/card_background"
            android:elevation="1dp"
            android:layout_margin="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="30dp"
/>
            <TextView
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="50dp"
                android:text="My Complaints"
                android:textColor="#66000000"
                android:textStyle="bold" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/myfir"
            android:layout_width="190dp"
            android:layout_height="90dp"
            android:background="@drawable/card_background"
            android:elevation="1dp"
            android:layout_margin="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/complaint" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="50dp"
                android:text="My FIR"
                android:gravity="center"
                android:textColor="#66000000"
                android:textStyle="bold"/>


        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="410dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/myservices"
            android:layout_width="190dp"
            android:layout_height="90dp"
            android:background="@drawable/card_background"
            android:elevation="1dp"
            android:layout_margin="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="30dp"
/>
            <TextView
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="50dp"
                android:text="My Services"
                android:textColor="#66000000"
                android:textStyle="bold" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mygrievence"
            android:layout_width="190dp"
            android:layout_height="90dp"
            android:background="@drawable/card_background"
            android:elevation="1dp"
            android:layout_margin="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="30dp"
/>
            <TextView
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="50dp"
                android:text="My Grievence"
                android:textColor="#66000000"
                android:textStyle="bold"/>


        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="410dp"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/mycyber"
            android:layout_width="190dp"
            android:layout_height="90dp"
            android:background="@drawable/card_background"
            android:elevation="1dp"
            android:layout_margin="8dp">

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="30dp"
/>
            <TextView
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="50dp"
                android:text="My Cyber Complaint"
                android:textColor="#66000000"
                android:textStyle="bold" />


        </RelativeLayout>

    </LinearLayout>

</LinearLayout>